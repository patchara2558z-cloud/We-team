for pageIndex, scriptList in pairs(scripts) do
    local page = pages[pageIndex]
    for i, scriptData in ipairs(scriptList) do
        local name, url = unpack(scriptData)
        local posY = 60 + (i - 1) * 40
        createButton(page, name, posY, function()
            pcall(function()
                loadstring(game:HttpGet(url, true))()
            end)
        end)
    end
end

local function addSwitchButtons(currentPage, backPage, nextPage)
    if backPage then
        local back = Instance.new("TextButton")
        back.Size = UDim2.new(0, 50, 0, 50)
        back.AnchorPoint = Vector2.new(1, 1)
        back.Position = UDim2.new(1, -70, 1, -10)
        back.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        back.Text = "<"
        back.TextColor3 = Color3.fromRGB(255, 255, 255)
        back.Font = Enum.Font.SourceSansBold
        back.TextScaled = true
        back.Parent = currentPage

        back.MouseButton1Click:Connect(function()
            currentPage.Visible = false
            backPage.Visible = true
        end)
    end

    if nextPage then
        local next = Instance.new("TextButton")
        next.Size = UDim2.new(0, 50, 0, 50)
        next.AnchorPoint = Vector2.new(1, 1)
        next.Position = UDim2.new(1, -10, 1, -10)
        next.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        next.Text = ">"
        next.TextColor3 = Color3.fromRGB(255, 255, 255)
        next.Font = Enum.Font.SourceSansBold
        next.TextScaled = true
        next.Parent = currentPage

        next.MouseButton1Click:Connect(function()
            currentPage.Visible = false
            nextPage.Visible = true
        end)
    end
end

addSwitchButtons(pages[1], nil, pages[2])
addSwitchButtons(pages[2], pages[1], pages[3])
addSwitchButtons(pages[3], pages[2], pages[4])
addSwitchButtons(pages[4], pages[3], pages[5])
addSwitchButtons(pages[5], pages[4], pages[6])
addSwitchButtons(pages[6], pages[5], pages[nli)
