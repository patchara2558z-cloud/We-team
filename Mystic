                            end
                            if game.Workspace:FindFirstChild("BuildingAreas") then
                                if Object:IsDescendantOf(game.Workspace.BuildingAreas) and Object.Name ~= "BasePlate" then
                                    Object.Anchored = false
                                    Object:BreakJoints()
                                end
                            end
                        end
                        Recurse(Object)
                    end
