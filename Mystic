local function main(m)
    m.Button1Down:connect(function(key)
        if test == true then
            test = false
            local function playMySound(Sound)
                if player.Character:FindFirstChild("AddOnNS") then
                    player.Character.AddOnNS:Remove()
                    local b= Instance.new("StringValue", player.Character)
                    b.Name = "AddOnNS"
                    local c= Instance.new("StringValue", b)
                    c.Name = "AddOnNS"
                    local a = Instance.new("Sound", c)
                    a.Name = "BlackHatHelper"
                    a.SoundId = "rbxassetid://" .. Sound
                    if Sound == "stop" then
                        a:Stop()
                        return
                    end
                    a.Volume = math.huge
                    a.Pitch = 1
                    a.Looped = true
                    a:Play()
                    
                    game:GetService('ReplicatedStorage').ArcsLocalRemote:FireClient(vars.Player,'CustomAssetSoundSwitch',a,tonumber(Sound))
                elseif not player.Character:FindFirstChild("AddOnNS") then
                    local b= Instance.new("StringValue", player.Character)
                    b.Name = "AddOnNS"
                    local c= Instance.new("StringValue", b)
                    c.Name = "AddOnNS"
                    local a = Instance.new("Sound", c)
                    a.Name = "BlackHatHelper"
                    a.SoundId = "rbxassetid://" .. Sound
                    if Sound == "stop" then
                        a:Stop()
                        return
                    end
                    a.Volume = math.huge
                    a.Pitch = 1
                    a.Looped = true
                    wait(0.3)
                    a:Play()
                    
                    game:GetService('ReplicatedStorage').ArcsLocalRemote:FireClient(vars.Player,'CustomAssetSoundSwitch',a,tonumber(Sound)) 
                end
            end
            
